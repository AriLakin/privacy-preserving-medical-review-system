{
  "name": "fhevm-anonymous-medical-review",
  "version": "1.0.0",
  "description": "FHEVM Example: Privacy-Preserving Medical Review System - Demonstrating access control, encryption, and public decryption patterns",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy.js --network sepolia",
    "deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
    "test": "npx hardhat test",
    "test:verbose": "npx hardhat test --verbose",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "coverage": "npx hardhat coverage",
    "scaffold": "node automation/create-fhevm-example.js",
    "generate-docs": "node automation/generate-docs.js",
    "node": "npx hardhat node"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
